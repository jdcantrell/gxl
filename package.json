{
  "name": "gxl",
  "version": "1.0.0",
  "description": "A css library of sorts",
  "main": "index.js",
  "scripts": {
    "css": "npm run mono",
    "mono": "node-sass ./src/mono/mono.sass -o build/css/",
    "gxl": "node-sass ./src/gxl/sass/gxl.sass --importer ./node_modules/compass-importer/index.js -o build/css/"
  },
  "keywords": [
    "css"
  ],
  "author": "JD Cantrell",
  "license": "GPL-3.0",
  "dependencies": {
    "compass-importer": "^0.4.1",
    "node-sass": "^4.5.2"
  }
}
